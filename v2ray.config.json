{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
        "tag": "socks-proxy",
        "port": 1089,
        "listen": "0.0.0.0",
        "protocol": "socks",
        "settings": {
            "auth": "noauth",
            "udp": true,
            "ip": "0.0.0.0",
            "userLevel": 0
        }
    },
    {
      "tag": "http-proxy",
      "port": 8080,
      "listen": "0.0.0.0",
      "protocol": "http",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "settings": {
        "auth": "noauth",
        "udp": true,
        "ip": "0.0.0.0"
      }
    }
  ],
  "outbound": {
    "protocol": "freedom"
  },
  "dns": {
    "servers": ["0.0.0.0", "1.1.1.1", "8.8.8.8", "8.8.4.4"]
  }
}
